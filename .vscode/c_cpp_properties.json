{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/coresim/",
                "${workspaceFolder}/ext/",
                "/usr/local/include/"
            ],
            "defines": [],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "${default}",
            "compilerArgs": [
                "-g -O3 -gdwarf-2 -Wall -std=c++0x"
            ]
        }
    ],
    "version": 4
}